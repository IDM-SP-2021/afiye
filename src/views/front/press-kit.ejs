<%- include('../partials/front-header') %>

<main id="press-kit-container">
  <div id="presskit-heading">
    <object
      id="hero"
      data="assets/blob-promo-presskit-hero.svg"
      type="image/svg+xml"
    ></object>

    <h1>Press Kit</h1>

    <div class="promo-text">
      <h2 class="promo-heading">Need Something?</h2>
      <p>
        Contact us at <br />
        ADD SOURCE HERE
      </p>
    </div>
  </div>

  <div id="about">
    <img src="https://via.placeholder.com/133.png" alt="team img" />
    <div class="promo-text">
      <h2 class="promo-heading">About Us</h2>
      <p>I need something to put here</p>
    </div>
  </div>

  <div id="brand-guide">
    <h2 class="promo-heading">Brand Guide & Visual Identity</h2>
    <img src="https://via.placeholder.com/345x480.png" alt="team img" />
    <button id="download-brand-eps" class="button primary">
      Download .eps
    </button>
    <button id="download-brand-pdf" class="button primary">
      Download .pdf
    </button>
  </div>

  <div id="media-gallery">
    <h2 class="promo-heading">Media Gallery</h2>
    <div id="faux-carousel">
      <button class="button icon"></button>
      <img src="https://via.placeholder.com/245x480.png" alt="team img" />
      <button class="button icon"></button>
    </div>

    <button id="download-media-zip" class="button primary">
      Download .zip
    </button>
  </div>

  <footer>
    <p id="copyright">&copy;afiye</p>
  </footer>
</main>
